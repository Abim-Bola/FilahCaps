<%- include ("partials/message"); %>

<div class="container" style="padding-left: 10%; padding-right: 10%; ">
  <div class="row" style="background-color: white; border: black 2px solid;">
      <div class="col-8">
          <table style="width:100%">
              <tr>
                <th>Product</th>
                <th>Price</th>
                <th>Quantity</th>
                <th>Total</th>
              </tr>
<% if(products) { %>

  <% for(var i = 0; i < products.length; i++) { %>

            <tr>
              <td>
                <img src="<%= products[i].item.image %>" alt="filahcaps" height="100px" width="100px">
                <p><%= products[i].item.productName %></p>
              </td>
              <td><%= products[i].item.price %></td>
              <td><%= products[i].qty %></td>
              <td><%= products[i].totalPrice %></td>
      
            </tr>
           
          
  <% } %>

<% } %>

</table>
</div>
<div class="col-4">col-4</div>
</div>
</div>

